<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:NavigationPage.ViewModels"
             xmlns:themes="clr-namespace:NavigationPage.Themes"
             xmlns:views="clr-namespace:NavigationPage.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="NavigationPage.Views.MainView"
             x:DataType="vm:MainViewModel">
    <themes:NavigationView Background="White">
        <themes:NavigationView.NavigtorContent>
            <themes:MagicBar ItemsSource="{Binding ContentVM}"/>
        </themes:NavigationView.NavigtorContent>
    </themes:NavigationView>
</UserControl>
